{
  "name": "whatsapp-shortcut-extension",
  "version": "1.0.0",
  "description": "Extensão para adicionar botões de atalho no WhatsApp Web",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ./src/compiled.css",
    "build": "npm run build:css && webpack --config webpack.config.js --mode=production",
    "build:dev": "npm run build:css && webpack --config webpack.config.js --mode=development",
    "watch": "npm run build:css && webpack --config webpack.config.js --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "frimousse": "^0.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.319",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  }
}
